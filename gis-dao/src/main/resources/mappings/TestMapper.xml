<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yugoo.gis.dao.ITestDAO">
    <select id="select" resultType="com.yugoo.gis.pojo.po.TestPO">
        SELECT * from tb_test limit 1
    </select>
    <update id="update">
        update tb_test set name=#{name}
    </update>
    <update id="uu">
        UPDATE tb_test set aa=#{aa}
    </update>
</mapper>