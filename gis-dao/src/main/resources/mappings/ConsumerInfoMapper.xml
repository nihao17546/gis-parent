<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yugoo.gis.dao.ConsumerInfoDAO">
    <select id="selectCount" resultType="java.lang.Long">
        select count(1) from tb_customer_info where 1=1
        <if test="startTime != null">
            and ctime &gt;= #{startTime}
        </if>
        <if test="endTime != null">
            and ctime &lt;= #{endTime}
        </if>
    </select>
    <select id="select" resultType="com.yugoo.gis.pojo.po.ConsumerInfoPO">
        select * from tb_customer_info where 1=1
        <if test="startTime != null">
            and ctime &gt;= #{startTime}
        </if>
        <if test="endTime != null">
            and ctime &lt;= #{endTime}
        </if>
        order by id desc
        limit ${offset},${rows}
    </select>
    <delete id="deleteById">
        delete from tb_customer_info where id=#{id}
    </delete>
</mapper>